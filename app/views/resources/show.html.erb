  <div class="container-lg">
    <div class="d-flex flex-column flex-md-row align-items-center justify-content-between mt-4">
      <h2>
        <%= @resources.first.group_info %>
      </h2>

      <div class="d-flex align-items-center justify-content-center gap-3">
        <div class="search-wrapper">
          <div class="position-relative">
            <input type="search" class="resource-search-input" placeholder="Quick Search..." id="search-input">
            <div class="search-icon">
              <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M23.3 21.3702L16.6486 14.7177C17.7205 13.2447 18.2971 11.4694 18.2954 9.64768C18.2954 4.87945 14.4159 1 9.64768 1C4.87945 1 1 4.87945 1 9.64768C1 14.4159 4.87945 18.2954 9.64768 18.2954C11.4694 18.2971 13.2447 17.7205 14.7177 16.6486L21.3702 23.3L23.3 21.3702ZM9.64768 15.5642C8.47736 15.5643 7.3333 15.2173 6.36017 14.5672C5.38704 13.9171 4.62856 12.9929 4.18065 11.9117C3.73274 10.8305 3.61552 9.64076 3.84381 8.49293C4.0721 7.34509 4.63565 6.29073 5.46319 5.46319C6.29073 4.63565 7.34509 4.0721 8.49293 3.84381C9.64076 3.61552 10.8305 3.73274 11.9117 4.18065C12.9929 4.62856 13.9171 5.38704 14.5672 6.36017C15.2173 7.3333 15.5643 8.47736 15.5642 9.64768C15.5623 11.2163 14.9384 12.7201 13.8292 13.8292C12.7201 14.9384 11.2163 15.5623 9.64768 15.5642Z" fill="black"/>
              </svg>
            </div>
          </div>
        </div>

        <% if false %>
          <button class="btn border">
            <svg width="24" height="23" viewBox="0 0 24 23" fill="none" xmlns="http://www.w3.org/2000/svg">
              <path d="M10.3333 16.3889H13.6667V14.7222H10.3333V16.3889ZM4.5 6.38892V8.05558H19.5V6.38892H4.5ZM7 12.2222H17V10.5556H7V12.2222Z" fill="#525866"/>
            </svg>
            Filter
          </button>
        <% end %>
      </div>
    </div>

    <div id="empty-results-message" class="d-none mt-4" style="height: 100vh;">
      <p>No results found</p>
    </div>
  </div>


  <div class="mt-4 resources-list container-lg">
    <%= render 'shared/page_font', verses: [@preview_verse] if is_quran_script %>

    <div class="card-container gap-4">
      <% @resources.each do |resource| %>
        <div class="resource-card tool-card-with-bg pattern<%= patterns.sample %>" data-search="<%= resource.name.to_s.downcase %> <%= resource.resource_type.to_s.downcase %> <%= resource.tags.to_s.downcase %> <%= resource.humanize_cardinality_type.to_s.downcase %>">
          <div class="p-4 h-100 d-flex flex-column justify-content-between">
            <div class="text-white">
              <h3 class="mb-0">
                <%= resource.name %>
              </h3>

              <small class="label label-info">
                <%= resource.humanize_cardinality_type %>
              </small>
            </div>

            <p class="text-white fw-bold">
              <%= resource.info %>
            </p>

            <% if is_quran_script %>
              <%= render 'quran_script_preview', resource: resource.resource_content, preview_verse: @preview_verse %>
            <% else %>
              <div class="icon">
                <i class="fa text-white <%= resource.icon %>" style="font-size: 5rem;"></i>
              </div>
            <% end %>

            <div class="d-flex w-100 align-items-center flex-column justify-content-center mt-5">
              <div class="dropdown mb-2">
                <a class="btn btn-primary  dropdown-toggle  w-100" href="#" role="button" id="dropdown-<%= resource.id %>" data-bs-toggle="dropdown" aria-expanded="false">
                  Select download format
                </a>

                <ul class="dropdown-menu" aria-labelledby="dropdown-<%= resource.id %>">
                  <% resource.downloadable_files.each do |file| %>
                    <li>
                      <% if user_signed_in? %>
                        <%= link_to resource_download_file_path(resource.resource_type, token: file.token), class: 'dropdown-item w-100' do %>
                          Download <%= file.file_type %>
                        <% end %>
                      <% else %>
                        <%= link_to '#_', class: 'dropdown-item w-100', data: { controller: 'ajax-modal', url: new_user_session_path(return_to: resource_download_file_path(resource.resource_type, token: file.token), modal: true)} do %>
                          Download <%= file.file_type %>
                        <% end %>
                      <% end %>
                    </li>
                  <% end %>
                </ul>
              </div>
            </div>

            <div>
              <% resource.get_tags.each do |tag| %>
                <span class="badge <%= resource_tag_class(tag.strip) %>">
                  <%= tag %>
                </span>
              <% end %>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
